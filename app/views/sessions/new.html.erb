<h1>Log In</h1>
<div class="form-container">
<%= form_for :login do |f| %>
  <div class="log_form">
    <%= f.label :email %>
    <%= f.text_field :email, autofocus: true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Log In" %>
  </div>
<% end %>
</div>